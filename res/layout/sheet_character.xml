				<TextView android:layout_height="wrap_content"
					android:text="10" android:id="@+id/gen_total_lp" style="@style/TextLabel"
					android:gravity="right|center_vertical" android:layout_width="60dip"></TextView>
			</TableRow>

			<TableRow android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<TextView android:layout_width="wrap_content" style="@style/TextLabel"
					android:layout_gravity="center_vertical" android:ellipsize="end"
					android:layout_height="fill_parent" android:gravity="center_vertical"
					android:text="@string/label_au"></TextView>
				<TextView android:layout_height="wrap_content"
