		<TextView android:layout_height="wrap_content" android:text="Beidhändig, Gezielter Stich"
			android:id="@+id/gen_specialfeatures" android:background="@color/Brighter"
			android:padding="5dip" android:inputType="textMultiLine"
			android:layout_marginTop="5dip" android:layout_width="fill_parent"
			android:layout_below="@+id/gen_attributes3" style="@style/TextLabelSmall"></TextView>




