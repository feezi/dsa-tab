				<TextView android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:text="10"
					android:id="@+id/gen_total_au" style="@style/TextLabel"
					android:gravity="right|center_vertical"></TextView>
			</TableRow>

			<TableRow android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<TextView android:layout_width="wrap_content" style="@style/TextLabel"
					android:id="@+id/gen_label_ae" android:layout_gravity="center_vertical"
					android:ellipsize="end" android:layout_height="fill_parent"
					android:gravity="center_vertical" android:text="@string/label_ae"></TextView>
				<TextView android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:text="10" style="@style/TextValue"
					android:id="@+id/gen_ae"></TextView>
				<TextView android:layout_height="wrap_content"
					android:layout_width="wrap_content" android:text="10"
